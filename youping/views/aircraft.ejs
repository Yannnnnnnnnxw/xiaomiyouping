<% include base/head.ejs %>

<% include base/nav.ejs %>

<% include base/nav1.ejs %>

<% include base/fixedbar.ejs%>

<style>
   *{
       padding:0px;
       margin:0px;
   }
   body{
       background:#f5f5f5;
   }
   .Box{
       width:1088px;
       height:920px;
       margin:0 auto;
   }
   .box{
       background:#fff;
       width:266px;
       height:267px;
       background:#fff;
       margin-left:5px;
       margin-bottom:5px;
       transition:0.5s
   }
   #tit{
       padding:0 90px;
       color:#333;
   }
   #tit span{
       color:#845f3f !important;
       font-size:14px;
   }
   .item-img{
       width:195px;
       height:195px;
       margin:0 auto;
   }
   .box p:first-of-type{
       color:#333;
       font-size:16px;
       text-align:center;
   }
   .box p:last-of-type{
       color:#A92112;
       font-size:14px;
       text-align:center;
   }
   .box p span{
       font-size:12px;
   }
   .Box .box{
       float: left;
       cursor: pointer;
   }

   .box:hover{
       transform:translateY(-2px);
       box-shadow:0  10px  10px #E9E9E9;
   }
</style>
<p id="tit">为您找到<span>11</span>条结果</p>
<div class="Box">
    <% for(var i=0;i<list.length;i++){ %>
    <div class="box">
        <div class="item-img">
            <img width="100%" src="/name/wyy/images/<%= list[i].img %>@base@tag=imgScale&F=webp">
        </div>
        <p><%= list[i].desc %></p>
        <p><span>￥</span><%= list[i].price %></p>
        
    </div>
    <% } %>
</div>
<% include base/nav2.ejs %>

