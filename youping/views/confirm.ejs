<script>
    var box=document.getElementsByClassName('box')[0]
    
    var boxImg=document.getElementById('boxImg')
    var address=document.getElementsByClassName('address')
    if (box) {
        box.onmouseover=function(){
            boxImg.innerHTML='<img src="/name/wyy/images/jiahao1.png">'
            newAddress.style.color='#845f3f'
        }
        box.onmouseleave=function(){
            box.style.border='1px solid #E7E7E7'
            boxImg.innerHTML='<img src="/name/wyy/images/jaihao.png">'
            newAddress.style.color='#E7E7E7'
        }
        box.onclick=function(){
            Eject.style.display='block'
            mask.style.display='block'
        }
    }
    
 
      //radio
      radio.onclick=function(){
          radio1.style.display='block'
          radio.style.display='none'
      }
      radio1.onclick=function(){
          radio.style.display='block'
          radio1.style.display='none'
      }
      //error
      var regexg = new RegExp('/^[0-9]{11}$/')
      var names= document.getElementById('names')
      var error_names= document.getElementById('error_names')
      var names_cha= document.getElementById('names_cha')
      names.onblur=function(){
          if(names.value==''){
            names.style.border='1px solid #D54843'
            error_names.style.display='block'
            names_cha.style.display='block'
          }
      }
      num.onblur=function(){
              if(!(num.value.length=='11')){
                    num.style.border='1px solid #D54843'
                    error_num.style.display='block'
                    num_cha.style.display='block'
            }else{
                    num.style.border='1px solid #eee'
                    error_num.style.display='none'
                    num_cha.style.display='none'
            }
      }
      street.onblur=function(){
        if(street.value==''){
            street.style.border='1px solid #D54843'
            error_street.style.display='block'
        }else{
            error_street.style.display='none'
        }
